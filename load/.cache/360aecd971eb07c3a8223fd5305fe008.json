Show()\n            } else {\n                this.panelHide()\n            }\n        })\n        // Excel载入\n        this.$uploader.on('change', () => {\n            const file = this.$uploader.get(0).files[0]\n            const reader = new FileReader()\n            reader.readAsText(file, 'utf-8')\n            reader.onload = e => {\n                this.ids = e.target.result.split(\"\\n\")\n                if (this.ids.length > 0) {\n                    this.$progress.show()\n                    this.$control.show()\n                }\n            }\n        })\n        // 开始按钮\n        this.$startButton.on('click', () => {\n            // this.finishLength ++\n            // this.progressUpdate()\n            this.finish.push({\n                name: 'liyang'\n            })\n            this.progressUpdate()\n        })\n    }\n    // 显示面板\n    panelShow () {\n        this.$panel.show()\n        this.$panelToggleBtn.text('隐藏')\n    }\n    // 隐藏面板\n    panelHide () {\n        this.$panel.hide()\n        this.$panelToggleBtn.text('显示')\n    }\n    // 更新进度条\n    progressUpdate () {\n        const n = Math.round(this.finish.length / this.ids.length * 100)\n        this.$progressBar.css('width', `${n}%`)\n    }\n}"},"lineCount":null}},"hash":"175d7c428d00a8f7169fdd42c427806e","cacheData":{"env":{}}}